{
  "routes": [
    {
      "route": "/api/*",
      "statusCode": 404
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/assets/*",
      "/*.css",
      "/*.js",
      "/*.ico",
      "/*.png",
      "/*.svg",
      "/*.json",
      "/*.txt",
      "/*.map"
    ]
  }
}
